<?xml version='1.0' encoding='utf-8'?>
<widget id="com.novatama.tokomobile" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="FileOpener">
        <param name="android-package" value="fr.smile.cordova.fileopener.FileOpener" />
    </feature>
    <feature name="Canvas2ImagePlugin">
        <param name="android-package" value="org.devgeeks.Canvas2ImagePlugin.Canvas2ImagePlugin" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Tokomobile</name>
    <author email="dev@cordova.apache.org" href="http://cordova.io">Novatama</author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Orientation" value="portrait" />
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <description>tokomobile apps</description>
    <icon src="img/icon.png" />
</widget>
